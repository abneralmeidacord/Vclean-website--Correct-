{
  "expo": {
    "name": "Vclean-website",
    "slug": "Vclean-website",
    "version": "1.0.0",
    "assets": [
      "./app/favicon.ico"
    ],
    "orientation": "portrait",
    "icon": "./app/favicon.ico",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/favicon.ico",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/favicon.ico",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
